{
  "name": "icebird",
  "version": "0.1.0",
  "description": "Apache Iceberg client for javascript",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "keywords": [
    "avro",
    "hyparquet",
    "iceberg",
    "parquet",
    "parquetjs",
    "parser",
    "snappy",
    "thrift"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/icebird.git"
  },
  "main": "src/iceberg.js",
  "files": [
    "src",
    "types"
  ],
  "type": "module",
  "scripts": {
    "build:types": "tsc -p ./tsconfig.build.json",
    "coverage": "vitest run --coverage --coverage.include=src",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "npm run build:types",
    "test": "vitest run"
  },
  "dependencies": {
    "avsc": "5.7.7",
    "fzstd": "0.1.1",
    "hyparquet": "1.9.0"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "@vitest/coverage-v8": "3.0.8",
    "eslint": "9.22.0",
    "eslint-plugin-jsdoc": "50.6.6",
    "typescript": "5.8.2",
    "vitest": "3.0.8"
  }
}
